<div class="banner-w3agile">
  <div class="container">
    <h3>
      <span>
        <h4><%= t ".subject_levels" %></h4>
      </span>
    </h3>
  </div>
</div>
<div class="containe">
  <div class="col-md-12 contact-left cont">
  <table class="table table-bordered">
    <thead class="thead-inverse">
      <tr>
        <th class="text-center"><%= t ".subject_name" %></th>
        <th class="text-center"><%= t ".name" %></th>
        <th class="text-center"><%= t ".question_number" %></th>
        <th class="text-center" colspan="2"><%= t ".actions" %></th>
      </tr>
    </thead>
    <tbody class="subject-index">
    <% @levels.each do |level| %>
      <tr>
        <td><%= @subject.name %></td>
        <td><%= link_to level.name, [:admin, level] %></td>
        <td><%= level.question_number %></td>
        <td class="text-center">
          <div class="btn-group" role="group">
          <%= link_to image_tag("edit.png",
            class:"img-responsi"), edit_admin_level_path(level.id) %>
          </div>
        </td>
        <td class="text-center">
          <div class="btn-group" role="group">
          <%= link_to image_tag("delete.png",
            class:"img-responsi"), admin_level_path(level.id),
              method: :delete,
              data: {confirm: t(".delete_subject_confirm")}%>
          </div>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
  </div>
  <div class="col-md-3 contact-left"></div>
  <div class="col-md-9 contact-left">
  </div>
</div>



