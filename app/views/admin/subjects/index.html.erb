<div class="banner-w3agile">
   <div class="container">
    <h3>
      <span>
        <%= link_to t(".new_subject"), new_admin_subject_path,
        remote: true, class: "btn btn-primarys" %>
      </span>
    </h3>
  </div>
</div>
<div class="containe">
  <div class="col-md-3 contact-left">
    <div class="contct-info">
      <h4> <%= t ".function" %></h4>
      <p>
        <%= t ".Admin" %>
      </p>
      <ul>
        <li><%= t ".name" %></li>
        <li><%= t ".duration" %></li>
      </ul>
    </div>
  </div>
  <div class="col-md-9 contact-left cont">
  <h4><%= t ".title" %></h4>
  <table  id="table_list" class="table table-striped table-bordered">
    <thead class="thead-inverse">
      <tr>
        <th class="text-center"><%= t ".name" %></th>
        <th class="text-center"><%= t ".duration" %></th>
        <th class="text-center" colspan="2"><%= t ".actions" %></th>
      </tr>
    </thead>
    <tbody class="subject-index">
      <%= render "subjects" %>
    </tbody>
  </table>
  </div>
  <div class="col-md-3 contact-left"></div>
  <div class="col-md-9 contact-left">
    <%= will_paginate @subjects %>
  </div>
  <div id="subject-modal" class="modal fade"></div>
</div>
